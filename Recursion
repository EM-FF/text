public class Recursion01{

	public static void main(String[] args){
		T t1 = new T();
		int res = t1.fibonacci(5);
		System.out.println(res);
	}
}

class T {

	public int fibonacci(int n){	

		int a = 1, b = 1;
		for(int j = 1; j < n; j++){
			int num = a + b;
			a = b;
			b = num;
		}
		return a;

	}
}
